javascript:var num=prompt("History flood amount: (100-1000) "); done = false; x = for (var i=1; i<=num; i++) {history.pushState(0, 0, i==num?x:i.toString()); if(i==num){done=true}}if(done===true){alert("History flood successful! "+" the tab now appears in your history "+num+(num==1?" time.":" times. (Made by Emaz)"))} 
